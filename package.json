{
  "name": "project.template.ts",
  "version": "0.0.1",
  "description": "Template for Typescript projects",
  "main": "app/app.ts",
  "scripts": {
    "test": "jest ./test",
    "start": "tsx ./app/app.ts",
    "dev": "ts-node-dev --respawn --transpile-only ./app/app.ts",
    "lint:fix": "biome format ./app/**  ./app/** --write",
    "lint": "biome format ./app/**  ./app/**",
    "build": "tsc",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "author": "pavel@p3ld3v.dev",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.1.2",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.1",
    "@types/node": "^20.5.0",
    "husky": "^8.0.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2"
  }
}
